{
  "name": "nosskey-sdk",
  "version": "0.0.0",
  "description": "SDK for Passkey-Derived Nostr Identity a.k.a. Nosskey",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "engines": {
    "node": "22.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocknamo/nosskey-sdk.git"
  },
  "keywords": [
    "nostr"
  ],
  "author": "ocknamo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocknamo/nosskey-sdk/issues"
  },
  "homepage": "https://github.com/ocknamo/nosskey-sdk#readme",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.14.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "volta": {
    "node": "22.15.0"
  }
}