{
  "name": "nosskey-sdk",
  "version": "0.0.2",
  "description": "SDK for Passkey-Derived Nostr Identity a.k.a. Nosskey",
  "files": [
    "dist",
    "LICENSE"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocknamo/nosskey-sdk.git"
  },
  "keywords": [
    "nostr"
  ],
  "author": "ocknamo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocknamo/nosskey-sdk/issues"
  },
  "homepage": "https://github.com/ocknamo/nosskey-sdk#readme",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "format": "biome format --write && biome check --write",
    "format:check": "biome check ./src",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "fix": "npm run lint:fix && npm run format",
    "fix:unsafe": "biome check --fix --unsafe",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.14.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "volta": {
    "node": "22.15.0"
  },
  "dependencies": {
    "rx-nostr": "^3.6.1",
    "rx-nostr-crypto": "^3.1.3"
  },
  "directories": {
    "doc": "docs",
    "example": "examples"
  }
}